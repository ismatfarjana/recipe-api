<main>
  <h1 id="title">Login</h1>
</main>

{{#if error }}
  <p>Error: {{ error }} </p>
{{/if}}

<form method="post" action="/users/login">
  <label for="email">Email:</label><br>
  <input type="text" id="email" name="email" value="Enter Email"><br><br>
  <label for="password">Password:</label><br>
  <input type="text" id="password" name="password" value="Enter password"><br><br>
  <input type="submit" value="Submit">
</form> 